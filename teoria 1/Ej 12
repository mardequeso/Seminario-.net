//12) Escribir un programa que imprima todos los divisores de un número entero ingresado desde la consola. Para obtener el entero
//desde un string st utilizar int.Parse(st).
Console.WriteLine("Ingrese un número: ");
int n = int.Parse(Console.ReadLine());
for (int i = 1; i <= n; i++){
    if (n % i == 0){
        Console.WriteLine(i);
    }
}
